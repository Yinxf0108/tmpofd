    <xs:complexType name="CT_LaGouraudShd">
        <xs:sequence>
            <xs:element name="Point" minOccurs="4" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Color" type="CT_Color"/>
