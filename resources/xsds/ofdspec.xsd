        <xs:attribute name="Value" type="ST_Array"/>
        <xs:attribute name="Index" type="xs:int"/>
        <xs:attribute name="ColorSpace" type="ST_RefID"/>
        <xs:attribute name="Alpha" type="xs:int" default="255"/>
    </xs:complexType>
